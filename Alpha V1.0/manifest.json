{
  "manifest_version": 3,
  "name": "TabSync Alpha",
  "version": "1.0",
  "description": "Alpha - Otomatik kaydet özelliği",
  "permissions": ["tabs", "storage", "alarms"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "512": "icons/icon512.png"
  }
}